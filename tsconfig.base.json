      "@hub/shared/feature/meal-search-autocomplete": [
        "libs/shared/feature/meal-search-autocomplete/src/index.ts"
      ],
      "@hub/shared/model/food-models": [
        "libs/shared/model/food-models/src/index.ts"
      ],
      "@hub/shared/testlib": ["libs/shared/testlib/src/index.ts"],
      "@hub/shared/ui/material": ["libs/shared/ui/material/src/index.ts"],
      "@hub/shared/ui/navigator": ["libs/shared/ui/navigator/src/index.ts"],
      "@hub/shared/ui/section": ["libs/shared/ui/section/src/index.ts"],
      "@hub/shared/ui/sideboard": ["libs/shared/ui/sideboard/src/index.ts"],
      "@hub/shared/ui/stepper": ["libs/shared/ui/stepper/src/index.ts"],
      "@hub/shared/util/app-config": [
        "libs/shared/util/app-config/src/index.ts"
